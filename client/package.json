{
  "name": "collection-manager-client",
  "version": "1.0.0",
  "private": true,
  "author": "Maikel van Veen <maikeloo10@hotmail.com>",
  "license": "MIT",
  "scripts": {
    "build": "yarn clear-dist && parcel build public/index.html --no-source-maps && yarn move-html-file",
    "clear-dist": "rm -rf dist/",
    "move-html-file": "mv dist/index.html ./index.html",
    "open-chrome": "wait-on http://localhost:1234 && open -a \"Google Chrome\" http://localhost:1234",
    "start-client": "yarn clear-dist && concurrently \"yarn watch-client\" \"yarn open-chrome\" --raw",
    "watch-client": "parcel public/index.html --no-source-maps"
  },
  "devDependencies": {
    "@types/classnames": "^2.2.6",
    "@types/graphql": "^14.0.3",
    "@types/lodash-es": "^4.17.1",
    "@types/react": "^16.7.17",
    "@types/react-dom": "^16.0.11",
    "@types/react-router-dom": "^4.3.1",
    "autoprefixer": "^9.4.2",
    "babel-core": "^6.26.3",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "concurrently": "^4.1.0",
    "json5": "^2.1.0",
    "parcel-bundler": "^1.10.3",
    "sass": "^1.15.2",
    "typescript": "^3.2.2",
    "wait-on": "^3.2.0"
  },
  "dependencies": {
    "@material-ui/core": "^3.6.2",
    "@material-ui/icons": "^3.0.1",
    "apollo-boost": "^0.1.22",
    "babel-polyfill": "^6.26.0",
    "classnames": "^2.2.6",
    "graphql": "^14.0.2",
    "graphql-tag": "^2.10.0",
    "lodash-es": "^4.17.11",
    "react": "^16.6.3",
    "react-apollo": "^2.3.3",
    "react-dom": "^16.6.3",
    "react-router-dom": "^4.3.1"
  }
}
